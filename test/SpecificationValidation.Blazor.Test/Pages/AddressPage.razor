@page "/addresspage"

<EditForm Model="Address" OnValidSubmit="Validate" OnInvalidSubmit=Validate>
    <SpecificationValidator Validator=Validator  />
    <AddressEditor Address=Address />
    <button class="btn btn-primary" type="submit">Enviar</button>
</EditForm>

@if(StatusMessage.Value is not null)
{
    <div class="alert alert-danger">@StatusMessage</div>
}


